---
const title = Astro.props.title;
---

<head>
  <!-- Required meta tags -->
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <!-- Favicon icon-->
  <link rel="shortcut icon" type="image/png" href="/assets/favicon.png" />

  <!-- Core Css -->
  <link rel="stylesheet" href="/backend/css/styles.css" />
  <link rel="stylesheet" href="/backend/libs/quill/dist/quill.snow.css" />
  <title>{title}</title>
  <!-- Owl Carousel  -->
  <link
    rel="stylesheet"
    href="/backend/libs/owl.carousel/dist/assets/owl.carousel.min.css"
  />
  <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/izitoast/dist/css/iziToast.min.css"
  />
  <!-- JS iziToast -->
  <script
    src="https://cdn.jsdelivr.net/npm/izitoast/dist/js/iziToast.min.js"
    defer></script>
</head>
